<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Service Level Agreements Data -->
        <record id="sla_1" model="scm.sla">
            <field name="name">Standard SLA</field>
            <field name="fulfillment_rate">95.0</field> <!-- Order fulfillment rate -->
            <field name="delivery_time">5</field> <!-- Maximum delivery time in days -->
            <field name="support_response_time">24</field> <!-- Maximum support response time in hours -->
