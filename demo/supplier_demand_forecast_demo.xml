<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="demand_forecast_1" model="scm.demand_forecast">
            <field name="inventory_item_id2" ref="scm.inventory_item_1"/> <!-- Reference to an existing inventory item -->
            <field name="scenario">best</field>
            <field name="period_start_date">2024-10-01</field>
            <field name="period_end_date">2024-10-31</field>
            <field name="forecasted_quantity">150</field>
            <field name="active" eval="True"/>
            <field name="supplier_id3" ref="scm.supplier_1"/> <!-- Reference to an existing supplier -->
        </record>
        
        <record id="demand_forecast_2" model="scm.demand_forecast">
