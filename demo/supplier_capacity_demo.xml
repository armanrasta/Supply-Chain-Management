<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <data noupdate="1">
    
        <record id="supplier_capacity1" model="scm.supplier_capacity">
            <field name="supplier_id5" ref="scm.supplier"/> <!-- Reference to an existing supplier -->
            <field name="product_id" ref="product.product"/> <!-- Reference to an existing product -->
            <field name="max_capacity">10000.00</field>
            <field name="min_capacity">5000.00</field>
            <field name="current_utilization">75.00</field>
            <field name="utilization_trend">5.00</field>
            <field name="lead_time">10</field>
            <field name="last_review_date">2024-10-01</field>
            <field name="capacity_type">normal</field>
            <field name="capacity_status">stable</field>
            <field name="period">monthly</field>
        </record>
        
        <record id="supplier_capacity2" model="scm.supplier_capacity">
            <field name="supplier_id5" ref="scm.supplier"/> <!-- Reference to another existing supplier -->
            <field name="product_id" ref="product.product"/> <!-- Reference to another existing product -->
            <field name="max_capacity">20000.00</field>
            <field name="min_capacity">10000.00</field>
            <field name="current_utilization">60.00</field>
            <field name="utilization_trend">-10.00</field>
            <field name="lead_time">15</field>
            <field name="last_review_date">2024-10-10</field>
            <field name="capacity_type">peak</field>
            <field name="capacity_status">decreasing</field>
            <field name="period">weekly</field>
        </record>
    
    </data>
    

</odoo>
